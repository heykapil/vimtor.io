{
    "name": "hello",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"next --port 3000\" \"cd studio && sanity start\"",
        "build": "echo 'Building Sanity to public/studio' && cd studio && sanity build ../public/studio -y && cd .. && next build",
        "start": "vercel start",
        "lint": "next lint",
        "prepare": "husky install",
        "format": "prettier . --write",
        "pre-commit": "lint-staged",
        "graphql": "graphql-codegen -r dotenv/config --watch",
        "postinstall": "lerna bootstrap"
    },
    "dependencies": {
        "@formspree/react": "^2.2.3",
        "@graphcms/rich-text-react-renderer": "^0.4.0",
        "@headlessui/react": "^1.4.2",
        "@heroicons/react": "^1.0.3",
        "@react-pdf/renderer": "^2.0.21",
        "@segment/snippet": "^4.15.3",
        "graphql": "^16.2.0",
        "graphql-request": "^3.7.0",
        "lodash": "^4.17.21",
        "next": "^12.0.7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-use": "^17.2.4"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@graphql-codegen/cli": "2.3.0",
        "@graphql-codegen/schema-ast": "^2.4.0",
        "@graphql-codegen/typescript": "^2.4.1",
        "@graphql-codegen/typescript-graphql-request": "^4.3.2",
        "@graphql-codegen/typescript-operations": "^2.2.1",
        "@tailwindcss/forms": "^0.4.0",
        "@types/lodash": "^4.14.172",
        "@types/marked": "^2.0.4",
        "@types/react": "^17.0.18",
        "autoprefixer": "^10.4.0",
        "eslint": "^7.32.0",
        "eslint-config-next": "^11.1.0",
        "husky": "^7.0.0",
        "lint-staged": "^11.1.2",
        "postcss": "^8.4.4",
        "prettier": "^2.3.2",
        "tailwindcss": "^3.0.1",
        "typescript": "4.3.5",
        "lerna": "^3.22.0",
        "concurrently": "^5.3.0",
        "@sanity/cli": "^2.0.5"
    }
}
